    android:orientation="vertical">

    <!--<include layout="@layout/toolbar" />-->
    <RelativeLayout
        android:id="@+id/rl_title_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/lightblue">

        <ImageView
            android:id="@+id/ivProductionBack"
            android:layout_width="@dimen/dimen_70"
            android:layout_height="@dimen/dimen_70"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_20"
            android:layout_marginRight="@dimen/dimen_10"
            android:src="@mipmap/ali_back_two" />

        <View
            android:id="@+id/viewtitletwo"
            android:layout_width="1px"
            android:layout_height="@dimen/dimen_100"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_30"
            android:layout_toRightOf="@+id/ivProductionBack"
            android:background="#FF909090" />

        <TextView
            android:id="@+id/tvProTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerInParent="true"
            android:text="生产日报"
            android:textColor="@color/icon_color"
            android:textSize="@dimen/dimen_55sp" />

        <ImageView
            android:id="@+id/ivSearch"
            android:layout_width="@dimen/dimen_70"
            android:layout_height="@dimen/dimen_70"
            android:layout_alignTop="@+id/spinnermenu"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_50"
            android:layout_marginStart="11dp"
            android:layout_toEndOf="@+id/viewtitletwo"
            android:src="@mipmap/ali_search_two" />

        <Button
            android:id="@+id/btnProSave"
            android:layout_width="@dimen/dimen_170"
            android:layout_height="@dimen/dimen_90"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/viewtitle"
            android:background="@drawable/shape_btn_newsbuild_boild"
            android:text="保存"
            android:textColor="@color/icon_color"
            android:visibility="gone" />

        <View
            android:id="@+id/viewtitle"
            android:layout_width="1px"
            android:layout_height="@dimen/dimen_100"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_30"
            android:layout_toLeftOf="@+id/spinnermenu"
            android:background="@color/title_view_boild"
            android:visibility="gone" />

        <Button
            android:id="@+id/spinnermenu"
            android:layout_width="@dimen/dimen_80"
            android:layout_height="@dimen/dimen_80"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dimen_30"
            android:layout_marginRight="@dimen/dimen_30"
            android:background="@mipmap/em_add"
            android:text="" />
    </RelativeLayout>

