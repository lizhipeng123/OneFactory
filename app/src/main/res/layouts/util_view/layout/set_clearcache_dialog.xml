<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dimen_960"
    android:layout_height="@dimen/dimen_529"
    android:background="@drawable/customdialog_bg"
    android:orientation="vertical">

    <TextView
        android:id="@+id/message"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_70"
        android:layout_marginTop="@dimen/dimen_141"
        android:gravity="center"
        android:text="he"
        android:textColor="#656565"
        android:textSize="20sp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginTop="@dimen/dimen_135"
        android:gravity="center">

        <Button
            android:id="@+id/cancelButton"
            android:layout_width="@dimen/dimen_520"
            android:layout_height="@dimen/dimen_132"
            android:layout_gravity="center"
            android:background="@drawable/gray_bg"
            android:text="取消"
            android:textColor="#696969"
            android:textSize="18sp" />
    </LinearLayout>
</LinearLayout>
